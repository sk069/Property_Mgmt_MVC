@model Property_Mgmt_MVC.Models.Dealer_Detail

@{
    ViewData["Title"] = "Create";
}

<h1>Create</h1>

<h4>Dealer Detail</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Dealer_Name" class="control-label">Dealer Name</label>
                <input asp-for="Dealer_Name" class="form-control" />
